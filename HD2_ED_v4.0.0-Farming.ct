local markSymbolNames = {
  "damage",
  "explosion",
  "proj",
  "arc",
  "beam",
  "strat",
  "status"
}

local finalSymbolNames = {}

for _, markSymbolName in ipairs(markSymbolNames) do
  if markSymbolName == "proj" or markSymbolName == "arc" or markSymbolName == "beam" then
    table.insert(finalSymbolNames, "address_" .. markSymbolName)
  else
    table.insert(finalSymbolNames, markSymbolName .. "Addr")
  end
end

for i, markSymbolName in ipairs(markSymbolNames) do
  local findSymbolName = markSymbolName .. "AddrFind"
  local findSymbol = _G[findSymbolName]
  local finalSymbolName = finalSymbolNames[i]
  _G[finalSymbolName] = findSymbol + readInteger(findSymbol + 3) + 7
end

local final_symbols = {
  'damageAddr',
  'explosionAddr',
  'address_proj',
  'address_arc',
  'address_beam',
  'stratAddr',
  'statusAddr'
}

for _, final_symbol in ipairs(final_symbols) do
  registerSymbol(final_symbol, _G[final_symbol], true)
end

local countBytesFinal = {
    damageAddr    = "countDamageMaxIDaddr",
    explosionAddr = "countExplosionMaxIDaddr",
    address_proj  = "countProjectileMaxIDaddr",
    address_arc   = "countArcMaxIDaddr",
    address_beam  = "countBeamMaxIDaddr",
    stratAddr     = "countStratMaxIDaddr",
    statusAddr    = "countStatusMaxIDaddr"
}

for scanKey, globalName in pairs(countBytesFinal) do
    _G[globalName] = getAddressSafe(scanKey)
    if _G[globalName] then
        _G[globalName] = _G[globalName] + 8
    else
        showMessage("Some of required addresses were not found!\nPlease restart the game.")
        error()
        return
    end
end

function getMaxID(countMaxIDAddresses)
  local maxID      = nil
  local lastID     = 0
  local countMaxID = 0

  while true do
    local IDpointer = readPointer(countMaxIDAddresses + 8 * countMaxID)
    local IDvalue   = readInteger(IDpointer)

    if not IDvalue or IDvalue &lt;= 0 then
      break
    end

    if IDvalue ~= lastID + 1 then
      break
    end

    maxID      = IDvalue
    lastID     = IDvalue
    countMaxID = countMaxID + 1
  end

  if maxID == nil then
    return 1
  else
    return maxID
  end
end

function calculateMaxIDs()
    maxDamageID     = getMaxID(countDamageMaxIDaddr)
    maxExplosionID  = getMaxID(countExplosionMaxIDaddr)
    maxProjectileID = getMaxID(countProjectileMaxIDaddr)
    maxArcID        = getMaxID(countArcMaxIDaddr)
    maxBeamID       = getMaxID(countBeamMaxIDaddr)
    maxStratID      = getMaxID(countStratMaxIDaddr)
    maxStatusID     = getMaxID(countStatusMaxIDaddr)
end

createThread(calculateMaxIDs)


end

local writeCCbytes = writeBytes(limitnewmem + 314, writeint3)

local function loopFunctionLimitsSC()

math.randomseed(os.time())

while SCLimitToken do
local uuid = random_uuid()
local byteArray = uuidToBytes(uuid)


local assemblyLimitsCode = assemblyLimitsCode .. [[
codelimit:
  reassemble(limitasm_bkp)
  reassemble(limitasm_bkp+3)
  jmp returnlimit
  align 10
limitscan:
  jmp limitnewmem
  nop 5
returnlimit:
]]
AutoAssemble(assemblyLimitsCode)
        sleep(1000)
    end
end

createThread(loopFunctionLimitsSC)

